<h1>Sign In</h1>
  <%= form_tag "sign_in" do %>
    <div class="f-label"><%= label_tag :email%></div>
    <div class="f-field"><%= text_field_tag :email %><div>
    <div class="f-label"><%= label_tag :password%><div>
    <div class="f-field"><%= password_field_tag :password %><div>
	<%= submit_tag "Log In" %>
  <% end %>